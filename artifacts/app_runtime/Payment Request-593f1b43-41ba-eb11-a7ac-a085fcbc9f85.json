						{
							"fieldNo": "08ff09a4-9394-4ef1-c850-534550b395a8",
							"fieldName": "GooglePayFunctions",
							"fieldParent": "c4cdea42-0fc6-4e93-9713-e7d2ed171765",
							"fieldType": "neptune.Script",
							"script": [
								"==OBJECT STRING==",
								"const allowedCardNetworks = [\"AMEX\", \"DISCOVER\", \"INTERAC\", \"JCB\", \"MASTERCARD\", \"VISA\"];\r",
								"const allowedCardAuthMethods = [\"PAN_ONLY\", \"CRYPTOGRAM_3DS\"];\r",
								"\r",
								"/**\r",
								" * Define your unique Google Pay API configuration\r",
								" *\r",
								" * @returns {object} data attribute suitable for PaymentMethodData\r",
								" */\r",
								"function getGooglePaymentsConfiguration() {\r",
								"    return {\r",
								"        environment: 'TEST',\r",
								"        apiVersion: 2,\r",
								"        apiVersionMinor: 0,\r",
								"        merchantInfo: {\r",
								"            // A merchant ID is available after approval by Google.\r",
								"            // 'merchantId':'12345678901234567890',\r",
								"            merchantName: 'Example Merchant'\r",
								"        },\r",
								"        allowedPaymentMethods: [{\r",
								"            type: 'CARD',\r",
								"            parameters: {\r",
								"                allowedAuthMethods: allowedCardAuthMethods,\r",
								"                allowedCardNetworks: allowedCardNetworks\r",
								"            },\r",
								"            tokenizationSpecification: {\r",
								"                type: 'PAYMENT_GATEWAY',\r",
								"                // Check with your payment gateway on the parameters to pass.\r",
								"                // @see {@link https://developers.google.com/pay/api/web/reference/request-objects#gateway}\r",
								"                parameters: {\r",
								"                    'gateway': 'example',\r",
								"                    'gatewayMerchantId': 'exampleGatewayMerchantId'\r",
								"                }\r",
								"            }\r",
								"        }]\r",
								"    };\r",
								"}\r",
								"\r",
								"/**\r",
								" * Create a PaymentRequest\r",
								" *\r",
								" * @returns {PaymentRequest}\r",
								" */\r",
								"function createGooglePaymentRequest() {\r",
								"    // Add support for the Google Pay API.\r",
								"    const methodData = [{\r",
								"        supportedMethods: 'https://google.com/pay',\r",
								"        data: getGooglePaymentsConfiguration()\r",
								"    }];\r",
								"    // Add other supported payment methods.\r",
								"    methodData.push({\r",
								"        supportedMethods: 'basic-card',\r",
								"        data: {\r",
								"            supportedNetworks:\r",
								"                Array.from(allowedCardNetworks, (network) => network.toLowerCase())\r",
								"        }\r",
								"    });\r",
								"\r",
								"    // const details = {\r",
								"    //     total: { label: 'Test Purchase', amount: { currency: 'USD', value: '1.00' } }\r",
								"    // };\r",
								"\r",
								"    const details = buildShoppingCartDetails();\r",
								"\r",
								"    const options = {\r",
								"        requestPayerEmail: true,\r",
								"        requestPayerName: true\r",
								"    };\r",
								"\r",
								"    return new PaymentRequest(methodData, details, options);\r",
								"}\r",
								"\r",
								"\r",
								"function canGooglePay() {\r",
								"    const request = createGooglePaymentRequest();\r",
								"    if (window.PaymentRequest) {\r",
								"        request.canMakePayment()\r",
								"            .then(function (result) {\r",
								"                if (result) {\r",
								"                    // Display PaymentRequest dialog on interaction with the existing checkout button\r",
								"                    oButtonGooglePay.setVisible(true);\r",
								"                    oButtonPay.setVisible(true);\r",
								"                }\r",
								"            })\r",
								"            .catch(function (err) {\r",
								"                sap.m.MessageToast.show(\r",
								"                    'Google Pay canMakePayment() error! ' + err.name + ' error: ' + err.message);\r",
								"            });\r",
								"    } else {\r",
								"        sap.m.MessageToast.show('PaymentRequest API not available.');\r",
								"    }\r",
								"}"
							],
							"request": [],
							"response": [],
							"attributes": [],
							"data": []
						},
						{
							"fieldNo": "06e66513-607e-4642-aa97-dec49d728f63",
							"fieldName": "PayFunctions",
							"fieldParent": "c4cdea42-0fc6-4e93-9713-e7d2ed171765",
							"fieldType": "neptune.Script",
							"script": [
								"==OBJECT STRING==",
								"function buildSupportedPaymentMethodData() {\r",
								"    // Example supported payment methods:\r",
								"    return [{\r",
								"        supportedMethods: 'basic-card',\r",
								"        data: {\r",
								"            supportedNetworks: ['visa', 'mastercard'],\r",
								"            supportedTypes: ['debit', 'credit']\r",
								"        }\r",
								"    }];\r",
								"}\r",
								"\r",
								"function createPaymentRequest() {\r",
								"    return new PaymentRequest(buildSupportedPaymentMethodData(),\r",
								"        buildShoppingCartDetails());\r",
								"}\r",
								"\r",
								"function buildShoppingCartDetails() {\r",
								"    var cartItem;\r",
								"    var cartItems = [];\r",
								"    var cartTotalAmt = 0;\r",
								"\r",
								"\r",
								"    $.each(modeloListProducts.oData, function (i, row) {\r",
								"        cartItem = {};\r",
								"        cartItem.label = row.product;\r",
								"        cartItem.amount = { currency: 'USD', value: row.price };\r",
								"        cartItems.push(cartItem);\r",
								"        cartTotalAmt += row.price;\r",
								"    })\r",
								"\r",
								"    var cartTotal = {\r",
								"        label: 'Total',\r",
								"        amount: { currency: 'USD', value: parseFloat(cartTotalAmt).toFixed(2) }\r",
								"    }\r",
								"\r",
								"    return {\r",
								"        id: 'order-123',\r",
								"        displayItems: cartItems,\r",
								"        total: cartTotal\r",
								"    };\r",
								"}\r",
								"\r",
								"\r",
								"function canPay() {\r",
								"    const request = createPaymentRequest();\r",
								"    if (window.PaymentRequest) {\r",
								"        request.canMakePayment()\r",
								"            .then(function (result) {\r",
								"                if (result) {\r",
								"                    // Display PaymentRequest dialog on interaction with the existing checkout button\r",
								"                    oButtonPay.setVisible(true);\r",
								"                }\r",
								"            })\r",
								"            .catch(function (err) {\r",
								"                sap.m.MessageToast.show(\r",
								"                    'canMakePayment() error! ' + err.name + ' error: ' + err.message);\r",
								"            });\r",
								"    } else {\r",
								"        sap.m.MessageToast.show('PaymentRequest API not available.');\r",
								"    }\r",
								"}"
							],
							"request": [],
							"response": [],
							"attributes": [],
							"data": []
						},
						{
							"fieldNo": "8cc310e8-1cbb-410a-c5b8-988de973892f",
							"fieldName": "init",
							"fieldParent": "c4cdea42-0fc6-4e93-9713-e7d2ed171765",
							"fieldType": "neptune.Script",
							"script": [
								"==OBJECT STRING==",
								"var productList =\r",
								"    [\r",
								"        { \"product\": null, \"price\": null },\r",
								"        { \"product\": \"Milk\", \"price\": 1.99 },\r",
								"        { \"product\": \"Eggs\", \"price\": 5.49 },\r",
								"        { \"product\": \"Bread\", \"price\": 1.69 },\r",
								"        { \"product\": \"Chocolate\", \"price\": 8.99 },\r",
								"        { \"product\": \"Wine\", \"price\": 18.99 }\r",
								"    ];\r",
								"\r",
								"\r",
								"sap.ui.getCore().attachInit(function () {\r",
								"\r",
								"    setTimeout(function () {\r",
								"        modeloSelectProducts.setData(productList);\r",
								"        modeloSelectProducts.refresh(true);\r",
								"        modeloListProducts.setData([]);\r",
								"        modeloListProducts.refresh();\r",
								"        canGooglePay();\r",
								"        canPay();\r",
								"\r",
								"        startApplePay();\r",
								"    }, 200);\r",
								"\r",
								"});"
							],
							"filePath": "init",
							"request": [],
							"response": [],
							"attributes": [],
							"data": []
						}
					]
